def connectbd():
    try:
        connection = cx_Oracle.connect(
            username,
            password,
            dsn,
            encoding = encoding
        )
        print('Conectado a la BD')
        cursor = connection.cursor()
        
        #Query Fase 1
        cursor.execute(queryT)
        rows = cursor.fetchall()
        print('')
        table = [rows[0],rows[1],rows[2],rows[3],rows[4]]
        header = ['Estado','Registrado','Procesado','Reproceso','Parametros']
        text = tabulate(header, table, tablefmt="fancy_grid")
        print(text)
        """for row in rows:
